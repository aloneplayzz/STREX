{
  "buildCommand": "tsx script/build-vercel.ts",
  "outputDirectory": "dist/public",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/assets/.*",
      "dest": "/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
